  object queryStates: TSQLQuery
    FieldDefs = <>
    Database = connectionMain
    Transaction = transactionMain
    SQL.Strings = (
      'select STATE_CODE, STATE_NAME'
      'from States'
      'where official_state = 1'
      '  AND country_code = :Country'
      'order by state_name'
      ''
    )
    Params = <    
